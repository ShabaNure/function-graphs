                <Grid>
                    <TextBox x:Name="In" BorderThickness="2" HorizontalAlignment="Center" VerticalAlignment="Center" Height="30" Margin="10,10,10,20" TextWrapping="Wrap"  Width="160" PreviewKeyDown="TextBox_KeyDown" />
